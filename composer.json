{
    "name": "niveshsaharan/laravel-passport",
    "description": "Laravel Passport Tool for Nova",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "laravel/nova": "^2.0",
        "laravel/passport": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "Niveshsaharan\\LaravelPassport\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Niveshsaharan\\LaravelPassport\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
